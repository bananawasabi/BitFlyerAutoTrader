{
	"log4js": {
		"appenders": {
			"system": {
				"type": "dateFile",
				"filename": "./system.log"
			},
			"console": {
				"type": "console"	
			},
			"stdout": {
				"type": "stdout"
			}
		},
		"categories": {
			"default": {
				"appenders": [
					"system", "console", "stdout"
				],
				"level": "INFO"
			}
		}
	}
}
